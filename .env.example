# WAHA WhatsApp HTTP API Configuration
# Copy this file to .env and fill in your actual values

# WAHA API Base URL (without trailing slash)
# Example: http://localhost:3000 or https://your-waha-instance.com
WAHA_BASE_URL=http://localhost:3000

# WAHA API Key
# This is the X-Api-Key header value required for authentication
WAHA_API_KEY=your-api-key-here

# WAHA Session Name
# Default session to use for WhatsApp operations
# Default: "default"
WAHA_SESSION=default
